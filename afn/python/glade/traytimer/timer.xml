            <child>
              <object class="GtkToggleButton" id="counting_up_button">
                <property name="label" translatable="yes">Counting Up</property>
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <property name="receives_default">True</property>
              </object>
            </child>
            <child>
